; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody",
"class": "Character",
"language": "GDScript",
"path": "res://Trees/Actors/Characters/Character.gd"
}, {
"base": "Reference",
"class": "NameGenerator",
"language": "GDScript",
"path": "res://Scripts/Utilities/NameGenerator.gd"
}, {
"base": "Area",
"class": "Stairs",
"language": "GDScript",
"path": "res://Trees/Worlds/LegacyWorld/Stairs/Stairs.gd"
}, {
"base": "Navigation",
"class": "World_Navigator",
"language": "GDScript",
"path": "res://Scripts/Core/WorldNavigation.gd"
} ]
_global_script_class_icons={
"Character": "",
"NameGenerator": "",
"Stairs": "",
"World_Navigator": ""
}

[application]

config/name="CernansPromise"
run/main_scene="res://Boot.tscn"
config/icon="res://icon.png"

[autoload]

WorldManager="*res://Scripts/Core/World/World.gd"
Log="*res://Scripts/Core/Logger.gd"
NodeUtilities="*res://Scripts/Core/Utilities.gd"
Options="*res://Scripts/Core/Options.gd"
Lobby="*res://Scripts/Core/Network/Lobby.gd"
InputMapping="*res://Scripts/Core/InputMapping.gd"
Hud="*res://Trees/Interface/Hud/Hud.tscn"
PauseMenu="*res://Trees/Interface/PauseMenu/PauseMenu.tscn"
MainMenu="*res://Trees/Interface/MainMenu/MainMenu.tscn"

[compression]

formats/zlib/compression_level=5
formats/gzip/compression_level=5

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/aspect="expand"

[editor]

convert_text_resources_to_binary_on_export=true

[editor_plugins]

enabled=PoolStringArray(  )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": 1,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

